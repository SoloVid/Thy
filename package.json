{
  "name": "thylang",
  "version": "0.1.0",
  "description": "Simple language for thy mobile programming",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --outfile=lib/index.js",
    "check-types": "tsc --noEmit",
    "clean": "node -p \"require('fs').rmSync('lib',{force:true,recursive:true})\"",
    "generate-declarations": "tsc --emitDeclarationOnly",
    "site:build": "node -r esbuild-register src/build/build-site.ts",
    "site:dev": "node -r esbuild-register src/build/dev-site.ts",
    "interpret": "node -r esbuild-register src/interpreter/cli.ts",
    "serve": "node -r esbuild-register src/editor/server.ts",
    "start": "node lib/index.js",
    "test": "uts -r esbuild-register src",
    "test:watch": "watchlist --eager src -- npm run test"
  },
  "author": "Grant Dennison",
  "license": "MIT",
  "dependencies": {
    "@babel/code-frame": "^7.16.7",
    "@stitches/react": "^1.2.8",
    "chalk": "^4.1.2",
    "glob": "^8.0.1",
    "lezer": "^0.13.0",
    "lz-string": "^1.5.0",
    "prismjs": "^1.29.0",
    "yargs": "^17.4.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1",
    "@types/babel__code-frame": "^7.0.3",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16",
    "@types/prismjs": "^1.26.0",
    "@types/yargs": "^17.0.10",
    "ace-builds": "^1.14.0",
    "chokidar": "^3.5.3",
    "esbuild": "~0.17",
    "esbuild-register": "^3.3.3",
    "expect": "^28.1.3",
    "markdown-to-jsx": "^7.3.2",
    "npm-run-all": "^4.1.5",
    "preact": "^10.11.3",
    "typescript": "~4.7",
    "under-the-sun": "^2",
    "watchlist": "^0.3.1"
  }
}
