{
    "name": "mobile-lang",
    "displayName": "Mobile Lang",
    "description": "Syntax highlighting for mobile lang",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mob",
            "aliases": ["Mobile Lang", "mob"],
            "extensions": [".moby"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mob",
            "scopeName": "source.moby",
            "path": "./syntaxes/mob.tmLanguage.json"
        }]
    }
}